{
  "expo": {
    "name": "ValorantHub",
    "slug": "ValorantHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dark",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0F1923",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.valoranthub.app"
    },
    "android": {
      "package": "com.valoranthub.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F1923"
      },
      "statusBar": {
        "backgroundColor": "#0F1923",
        "barStyle": "light-content"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0F1923"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "99f48038-78a8-43e9-a904-9dabda4a38ed"
      }
    },
    "owner": "clis3n"
  }
}
